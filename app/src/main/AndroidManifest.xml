<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snap_develop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TimelineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ApprovalPendingFollowListActivity" />
        <activity android:name=".activity.ApplicatedFollowListActivity" />
        <activity android:name=".activity.AuthActivity"></activity>
        <activity android:name=".activity.CommentActivity" />
        <activity android:name=".activity.DisplayCommentActivity" />
        <activity android:name=".activity.FollowerListActivity" />
        <activity android:name=".activity.FollowingListActivity" />
        <activity
            android:name=".activity.MapActivity"
            android:label="@string/title_activity_map" />
        <activity android:name=".activity.PostActivity" />
        <activity android:name=".activity.PostSearchActivity" />
        <activity android:name=".activity.SettingActivity" />
    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

</manifest>